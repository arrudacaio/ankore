{
  "name": "ankore",
  "version": "1.0.0",
  "description": "Ankore CLI para criar cards de sentence mining para Anki",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "ankore": "src/cli.js"
  },
  "scripts": {
    "start": "node src/cli.js start",
    "watch": "node src/cli.js start --watch",
    "sample-export": "node src/cli.js sample-export",
    "check": "node --check src/cli.js && node --check src/index.js && node --check src/modes/mining/index.js"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.8.6",
    "chalk": "^5.6.2",
    "clipboardy": "^4.0.0",
    "cli-table3": "^0.6.5",
    "figures": "^6.1.0",
    "ora": "^8.2.0",
    "supports-color": "^10.2.2"
  },
  "keywords": [
    "anki",
    "sentence-mining",
    "cli"
  ],
  "author": "",
  "license": "MIT"
}
